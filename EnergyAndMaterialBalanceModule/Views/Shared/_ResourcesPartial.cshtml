@model IEnumerable<Resources>
    <div class="row mt-3 mb-3">
        <label class="col-3 col-form-label" for="resources">Resource</label>
        <div class="col-9">
            <select class="form-control" id="resources">
                @foreach (var item in Model)
                {
                    <option>
                        @Html.DisplayFor(modelItem => item.ResourceName)
                    </option>
                }
            </select>
        </div>
    </div>
